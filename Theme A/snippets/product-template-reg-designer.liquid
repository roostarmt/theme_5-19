                              <li style="background-color: #073EFF;" data-bg="073EFF"></li>
                            </ul>
                          </div>

                          <div class="input-group"> 
                            <label for="properties-disable-outline">
                            <input type="checkbox" value="no outline" name="properties[_DisableOutline]" id="properties-disable-outline" class="doogma-disableoutline">
                            Disable Text Outline</label>
                            </div>
                          
                          <div class="input-group"> 
                            <label for="properties-number-size">Adjust number size</label>
                            <input type="range" min="10" max="200" default="50" name="properties[_NumberSize]" id="properties-number-size" class="doogma-numbersize">
                          </div>

                          <div class="input-group"> 
                            <label for="properties-spacing">Adjust spacing between letters</label>
                            <input type="range" min="-10" max="10" default="0" name="properties[_Spacing]" id="properties-spacing" class="doogma-spacing">
                          </div>
                          

